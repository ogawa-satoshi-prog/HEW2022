<!DOCTYPE html>
<html lang="ja" class="h-100">

<head>
  <title>Quest Meeting管理</title>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet"
    integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">
  <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js"
    integrity="sha384-ka7Sk0Gln4gmtz2MlQnikT1wXgYsOg+OMhuP+IlRH9sENBO0LRn5q+8nbTov4+1p"
    crossorigin="anonymous"></script>
  <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.6.0/jquery.min.js"></script>
  <script src="/socket.io/socket.io.js"></script>
</head>

<body class="d-flex flex-column h-100">
  <!-- HEADER -->
  <header>
    <nav class="navbar navbar-expand-lg navbar-dark bg-dark">
      <div class="container-fluid w-75">
        <a class="navbar-brand" href="#">QuestMeeting管理用</a>
        <div class="collapse navbar-collapse" id="navbarNavAltMarkup">
          <div class="navbar-nav">
            <a class="nav-link" href="#">Home</a>
          </div>
        </div>
        <div class="form-check form-switch">
          <label class="form-check-label text-light" for="guestEntryBtn">ゲスト登録</label>
          <input class="form-check-input" type="checkbox" id="guestEntryBtn">
        </div>
      </div>
    </nav>
  </header>

  <!-- MAIN -->
  <main class="mt-5">
    <section class="container w-50 mb-5">
      <h1 class="m-auto text-center mb-3">ログインユーザー</h1>
    </section>
    <section class="container w-50 mb-5">
      <h2 class="m-auto text-center mb-3">教員</h2>
      <!-- 教員 -->
      <table class="table table-striped">
        <thead>
          <tr>
            <th scope="col">id</th>
            <th scope="col">name</th>
          </tr>
        </thead>
        <tbody id="teachersTable">
        </tbody>
      </table>
    </section>
    <section class="container w-50 mb-5">
      <h2 class="m-auto text-center mb-3">生徒</h2>
      <!-- 生徒 -->
      <table class="table table-striped">
        <thead>
          <tr>
            <th scope="col">id</th>
            <th scope="col">name</th>
            <th scope="col">kick</th>
            <th scope="col">blackList</th>
            <th scope="col">waitClick</th>
          </tr>
        </thead>
        <tbody id="studentsTable">
        </tbody>
      </table>
    </section>
  </main>

  <!-- FOOTER -->
  <footer class="footer mt-auto py-3 bg-light">
    <div class="container text-center">
      <span class="text-muted">モロコシ商事</span>
    </div>
  </footer>
  <!-- JavaScript -->
  <script src="./js/back.js"></script>
</body>

</html>